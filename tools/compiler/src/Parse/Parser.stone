use Stone.Basic; 


space Stone.Analysis {
	
	public struct Parser {
		private Lexer* lexer = null;
		private ref Syntax syntax = null;

	}
	public Parser::this(ref Syntax syntax) {
			this.syntax = syntax; 
			this.lexer = new Lexer();
	}
	public Parser::~this() {
		if(lexer){
			free lexer;
		}
	}

}

